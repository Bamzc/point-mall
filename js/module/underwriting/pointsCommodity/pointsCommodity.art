{{each list}}
    <li class="mui-table-view-cell mui-media">
		<a href="#/cydetail">
			<img class="mui-media-object mui-pull-left" src="{{imgbaseurl}}{{$value.thumbnail}}">
			<div class="mui-media-body">
				<p class="cy-name">{{$value.commodityname}}</p>
				<p class="cy-points"><i>{{$value.integration}}</i>积分</p>
				<p class="cy-market">市场参考价：<i>{{$value.price}}元</i></p>
			</div>
		</a>
	</li>
{{/each}}